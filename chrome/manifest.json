{
    "name": "visrepo",
    "description": "getting to visfo repos...the lazy way",
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "images/16x16.png",
        "32": "images/32x32.png",
        "48": "images/48x48.png",
        "128": "images/128x128.png"
    },
    "omnibox": {
        "keyword": "v"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ]
}
